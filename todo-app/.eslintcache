ckgroundColor: theme.palette.background.paper,\n        border: '2px solid #000',\n        boxShadow: theme.shadows[5],\n        padding: theme.spacing(2, 4, 3),\n    },\n}))\n\n\nfunction Todo(props) {\n    const [open, setOpen] = useState(false);\n    const classes = useStyles();\n    const [input, setInput] = useState();\n\n    const handleOpen = () => {\n        setOpen(true);\n    }\n\n    const updateTodo =() => {\n        //update todo with ne text\n\n        db.collection('todos').doc(props.todo.id).set({\n            todo: input\n        }, { merge: true });\n\n        setOpen(false);\n    }\n    return (\n        <>\n        <Modal\n            open={open}\n            onClose={e => setOpen(false)}\n        >\n            <div className={classes.paper}>\n                <h1>open</h1>\n                <input placeholder={props.todo.todo} value={input} onChange={event => setInput(event.target.value)}/>\n                <Button onClick={updateTodo}>\n                    Update Todo\n                </Button>\n            </div>\n        </Modal>\n\n        <List className=\"todo_list\">\n            <ListItem>\n                <ListItemAvatar>\n                    <Avatar>\n                    </Avatar>\n                </ListItemAvatar>\n                <ListItemText primary={props.todo.todo} secondary=\"Deadline\" />\n            </ListItem> \n            <button onClick={e => setOpen(true)}>Edit</button>\n            <DeleteForeverIcon onClick={event => db.collection('todos').doc(props.todo.id).delete()}/>\n        </List>\n        </>\n    )\n}\n\nexport default Todo\n","/home/simant/Projects/reactFirebase/todo-app/src/firebase.js",[],{"ruleId":"24","severity":1,"message":"25","line":26,"column":11,"nodeType":"26","messageId":"27","endLine":26,"endColumn":21},"no-unused-vars","'handleOpen' is assigned a value but never used.","Identifier","unusedVar"]